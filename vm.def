NOP {
  NEXT
}

VAR (NAME, TYPE) {
  %var_init($0, $1);
  NEXT
}

SET (NAME, VALUE) {
  %var_setv($0, $1);
  NEXT
}

INIT (ID|NAME, TYPE) {
  %fnc_init($0, $1);
  NEXT
}

SEND (NAME|VALUE) {
  %fnc_send($1);
  NEXT
}

EXEC {
  %fnc_call();
  NEXT
}

DEL (NAME) {
  %var_free($0);
  NEXT
}

END {
  HALT
}
